{
  "name": "wetransfer/php-sdk",
  "type": "library",
  "description": "WeTransfer PHP SDK",
  "keywords": [
    "wetransfer",
    "transfer",
    "files",
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/arkaitzgarro/wetransfer-php-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Arkaitz Garro",
      "email": "arkaitz.garro@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "guzzlehttp/guzzle": "^5.3",
    "ramsey/uuid": "^3.7",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "require-dev": {
    "guzzle/plugin-mock": "^3.9",
    "phpunit/phpunit": "~5.0",
    "satooshi/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "WeTransfer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WeTransfer\\Test\\": "tests/"
    }
  },
  "bin": ["bin/wetransfer"]
}
